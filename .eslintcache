</div>\n                <div><img className=\"image\" src = {this.state.result.image} /></div>\n                <br></br>\n                <div className='score-section'>{this.state.result.description}</div>\n\n            </div>\n        );\n    }\n}","C:\\Users\\q\\react\\cs496_week3\\src\\App.js",[],"C:\\Users\\q\\react\\cs496_week3\\src\\common\\header.js",[],["45","46"],"C:\\Users\\q\\react\\cs496_week3\\src\\common\\footer.js",[],{"ruleId":"47","severity":1,"message":"48","line":3,"column":10,"nodeType":"49","messageId":"50","endLine":3,"endColumn":26},{"ruleId":"47","severity":1,"message":"51","line":3,"column":28,"nodeType":"49","messageId":"50","endLine":3,"endColumn":42},{"ruleId":"52","replacedBy":"53"},{"ruleId":"54","replacedBy":"55"},{"ruleId":"56","severity":1,"message":"57","line":53,"column":69,"nodeType":"58","messageId":"59","endLine":53,"endColumn":71},{"ruleId":"60","severity":1,"message":"61","line":54,"column":28,"nodeType":"62","messageId":"63","endLine":54,"endColumn":30},{"ruleId":"56","severity":1,"message":"57","line":59,"column":71,"nodeType":"58","messageId":"59","endLine":59,"endColumn":73},{"ruleId":"60","severity":1,"message":"61","line":60,"column":28,"nodeType":"62","messageId":"63","endLine":60,"endColumn":30},{"ruleId":"64","severity":1,"message":"65","line":93,"column":23,"nodeType":"66","endLine":93,"endColumn":66},{"ruleId":"64","severity":1,"message":"65","line":39,"column":22,"nodeType":"66","endLine":39,"endColumn":79},{"ruleId":"52","replacedBy":"67"},{"ruleId":"54","replacedBy":"68"},"no-unused-vars","'MuiThemeProvider' is defined but never used.","Identifier","unusedVar","'createMuiTheme' is defined but never used.","no-native-reassign",["69"],"no-negated-in-lhs",["70"],"array-callback-return","Array.prototype.map() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","jsx-a11y/alt-text","img